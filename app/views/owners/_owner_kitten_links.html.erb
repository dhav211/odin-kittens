<div class="owner-profile-kitten-grid">
  <% @owner.kittens.each do |kitten| %>
    <div class="owner-kitten-profile-card">
      <%= link_to(image_tag(kitten.profile_picture, class: "owner-profile-kitten-picture"), owner_kitten_path(@owner, kitten)) %>
      <%= kitten.name %>
    </div>
  <% end %>
</div>